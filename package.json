{
  "name": "server-manager-monorepo",
  "private": true,
  "workspaces": [
    "agent",
    "common"
  ],
  "scripts": {
    "dev": "concurrently \"npm:dev:manager\" \"npm:dev:agent\"",
    "dev:manager": "npm run dev --prefix manager",
    "dev:agent": "npm run dev -w agent",
    "build": "concurrently \"npm:build:workspaces\" \"npm:build:manager\"",
    "build:workspaces": "npm run build --workspaces --if-present",
    "build:manager": "npm run build --prefix manager"
  },
  "devDependencies": {
    "commonjs-to-es-module-codemod": "^0.5.7",
    "concurrently": "^9.2.1"
  },
  "dependencies": {
    "jscodeshift": "^17.3.0"
  }
}
